{
  "name": "peerjs",
  "version": "1.3.2",
  "description": "PeerJS client",
  "main": "./dist/peerjs.min.js",
  "homepage": "https://github.com/peers/peerjs#readme",
  "keywords": [
    "peerjs",
    "webrtc",
    "p2p",
    "rtc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peers/peerjs.git"
  },
